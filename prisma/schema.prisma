datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String @id @default(cuid())
  email     String @unique
  username  String @unique
  password  String
  createdAt DateTime @default(now()) @map("created_at")

  @@map("users")
}